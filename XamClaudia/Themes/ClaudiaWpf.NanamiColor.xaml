<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
					xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

	<LinearGradientBrush x:Key="ブラシ_髪"
						 EndPoint="0.5,1"
						 StartPoint="0.5,0">
		<GradientStop Color="#FF286DAE" />
		<GradientStop Color="#FF0C3E83"
					  Offset="1" />
	</LinearGradientBrush>
	<Color x:Key="色_髪_輪郭">Black</Color>
	<SolidColorBrush x:Key="ブラシ_髪_輪郭"
					 Color="{DynamicResource 色_髪_輪郭}" />
	<LinearGradientBrush x:Key="ブラシ_髪_影"
						 EndPoint="0.5,1"
						 StartPoint="0.5,0">
		<GradientStop Color="#FF102A63"
					  Offset="0" />
		<GradientStop Color="#FF040E43"
					  Offset="1" />
	</LinearGradientBrush>
	<LinearGradientBrush x:Key="ブラシ_肌"
						 EndPoint="0.5,1"
						 StartPoint="0.5,0">
		<GradientStop Color="#FFFDEADB"
					  Offset="1" />
		<GradientStop Color="#FFFDEAD9" />
	</LinearGradientBrush>
	<SolidColorBrush x:Key="ブラシ_肌_影"
					 Color="#FFF1C6B9" />
	<Color x:Key="色_肌_輪郭">Black</Color>
	<SolidColorBrush x:Key="ブラシ_肌_輪郭"
					 Color="{DynamicResource 色_肌_輪郭}" />
	<LinearGradientBrush x:Key="ブラシ_肌_影2"
						 EndPoint="0.5,1"
						 StartPoint="0.5,0">
		<GradientStop Color="#FFE1A292" />
		<GradientStop Color="#FFEDB9AC"
					  Offset="1" />
	</LinearGradientBrush>
	<SolidColorBrush x:Key="ブラシ_セーター"
					 Color="#FFA1DCFE" />
	<SolidColorBrush x:Key="ブラシ_セーター_輪郭"
					 Color="Black" />
	<SolidColorBrush x:Key="ブラシ_セーター_影"
					 Color="#FF7AB6FF" />
	<SolidColorBrush x:Key="ブラシ_シャツ"
					 Color="#FFF2FDFF" />
	<SolidColorBrush x:Key="ブラシ_シャツ_輪郭"
					 Color="Black" />
	<SolidColorBrush x:Key="ブラシ_影"
					 Color="#3F000040" />
	<SolidColorBrush x:Key="ブラシ_シャツ_影"
					 Color="#FFB5E1FE" />
	<SolidColorBrush x:Key="ブラシ_イヤリング_耳あて"
					 Color="#FFFFFFFE" />
	<SolidColorBrush x:Key="ブラシ_イヤリング_耳あて_輪郭"
					 Color="Black" />
	<RadialGradientBrush x:Key="ブラシ_イヤリング">
		<GradientStop Color="#FF37B4F0"
					  Offset="1" />
		<GradientStop Color="#FFDD5324"
					  Offset="0.66" />
		<GradientStop Color="#FF6DD713"
					  Offset="0.33" />
		<GradientStop Color="#FFF9C51F" />
	</RadialGradientBrush>
	<LinearGradientBrush x:Key="ブラシ_虹彩"
						 EndPoint="0.5,1"
						 MappingMode="RelativeToBoundingBox"
						 StartPoint="0.5,0">
		<GradientStop Color="#FF66C4A7"
					  Offset="0" />
		<GradientStop Color="#FFE0D480"
					  Offset="1" />
		<GradientStop Color="#FF70BCDE"
					  Offset="0.524" />
	</LinearGradientBrush>
	<SolidColorBrush x:Key="ブラシ_瞳孔"
					 Color="#FF0281E0" />
	<SolidColorBrush x:Key="ブラシ_ハイライト"
					 Color="White" />
	<SolidColorBrush x:Key="ブラシ_瞼"
					 Color="Black" />
	<SolidColorBrush x:Key="ブラシ_虹彩_輪郭"
					 Color="Black" />
	<LinearGradientBrush x:Key="ブラシ_瞳_影"
						 EndPoint="0.5,1"
						 StartPoint="0.5,0">
		<GradientStop Color="#33000000"
					  Offset="1" />
		<GradientStop Color="#CC000000" />
	</LinearGradientBrush>
	<SolidColorBrush x:Key="ブラシ_メガネ_フレーム"
					 Color="#FF014CA9" />
	<LinearGradientBrush x:Key="ブラシ_メガネ_レンズ"
						 EndPoint="0.5,1"
						 StartPoint="0.5,0">
		<GradientStop Offset="1" />
		<GradientStop Color="#7FFFFFFF"
					  Offset="0.5" />
	</LinearGradientBrush>
	<SolidColorBrush x:Key="ブラシ_口"
					 Color="#FFF4AD9A" />
	<LinearGradientBrush x:Key="ブラシ_口_輪郭"
						 EndPoint="0.5,1"
						 StartPoint="0.4,0">
		<GradientStop Color="Black"
					  Offset="0.6" />
		<GradientStop Offset="0.8" />
		<GradientStop Color="#00000000" />
		<GradientStop Color="Black"
					  Offset="0.353" />
	</LinearGradientBrush>
	<SolidColorBrush x:Key="ブラシ_髪_光"
					 Color="#BFFFFFFF" />

</ResourceDictionary>